<!--pages/goods_detail/goods_detail.wxml-->
<view class="wxyg_goods_detail">
  <!-- 轮播图 -->
  <view class="detail_swiper">
    <swiper
      autoplay
      circular
      indicator-dots
    >
      <swiper-item
        wx:for="{{ goodsDetail.pics }}"
        wx:key="pics_id"
      >
        <image mode="widthFix" src="{{ item.pics_mid }}"></image>
      </swiper-item>
    </swiper>
  </view>
</view>
