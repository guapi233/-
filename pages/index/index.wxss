/**index.wxss**/
.wxyg_index {
  padding-bottom: 20rpx;
}

.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}

.index_cate navigator image {
  width: 100%;
}

.index_floor .floor_group .floor_title {
  width: 100%;
  padding: 10rpx 0;
}

.index_floor .floor_group .floor_title image {
  width: 100%;
}

.floor_list {
  overflow: hidden;
}

.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  border-left: 10rpx solid #fff;
}

.index_floor .floor_group .floor_list navigator:first-child {
  height: 55.45422414vw;
}

.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
}

.floor_list navigator:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}

.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
  border-right: 10rpx solid #fff;
}

.floor_list navigator:nth-child(5) {
  border-right: 10rpx solid #fff;
}

.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}